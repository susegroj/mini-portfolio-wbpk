{
  "name": "mini-portfolio",
  "version": "1.0.0",
  "description": "A minimal portfolio with Vanilla JavaScript using webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve --config webpack.config.dev.js",
    "start": "webpack --config webpack.config.dev.js",
